{
  "name": "ng2wp",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "prod": "webpack --progress --profile --bail -p --config=./config/webpack-prod.config.js",
    "bs": "browser-sync start --config=./config/browser-sync.js",
    "build": "npm-run-all rm:dist prod",
    "serve": "npm-run-all build bs",
    "rm:cache": "npm cache clean",
    "rm:dist": "rimraf dist doc",
    "rm:npm": "rimraf node_modules",
    "clean": "npm-run-all rm:*",
    "clean-install": "npm run clean && npm i",
    "clean-start": "npm run clean-install && npm run start",
    "wp-watch": "webpack --watch --progress --profile --config=./config/webpack-dev.config.js",
    "wp-dev": "webpack --progress --profile --bail --config=./config/webpack-dev.config.js",
    "dev": "npm-run-all rm:dist wp-dev",
    "lint": "tslint --force \"src/**/*.ts\"",
    "pretest": "npm run lint",
    "watch-tests": "karma start ./config/karma.conf.js --no-single-run --auto-watch",
    "test": "karma start ./config/karma.conf.js",
    "e2e-live": "protractor ./config/protractor.conf.js --elementExplorer",
    "e2e": "protractor ./config/protractor.conf.js",
    "ci": "npm-run-all e2e test",
    "docs": "typedoc --options typedoc.json src/app/app.component.ts",
    "server": "webpack-dev-server --inline --progress --port 8000 --config=./config/webpack-dev.config.js",
    "start": "npm run server",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "typings:clean": "rimraf typings*",
    "typings:init": "typings init",
    "typings:da": "typings install dt~jasmine -DG",
    "typings:sa": "typings install dt~es6-shim dt~node -SG",
    "postinstall": "npm-run-all webdriver-update typings:*"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "bootstrap": "^3.3.6",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.2",
    "browser-sync": "^2.12.10",
    "codelyzer": "0.0.19",
    "copy-webpack-plugin": "^3.0.0",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.8.1",
    "http-proxy-middleware": "^0.15.2",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.3.4",
    "jasmine-spec-reporter": "^2.4.0",
    "json-loader": "^0.5.3",
    "karma": "0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.3",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-remap-istanbul": "0.0.6",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.7.0",
    "node-sass": "^3.4.2",
    "npm-run-all": "^2.1.1",
    "null-loader": "0.1.1",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-loader": "^0.9.1",
    "protractor": "^3.1.1",
    "raw-loader": "0.5.1",
    "remap-istanbul": "^0.6.4",
    "rimraf": "^2.5.1",
    "sass-loader": "^3.1.2",
    "shelljs": "^0.7.0",
    "style-loader": "^0.13.0",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^0.8.1",
    "tslint": "^3.4.0",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.0",
    "typings": "^1.0.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
